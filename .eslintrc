{
    "env": {
        "commonjs": true,
        "node":true,
        "es6":true
    },
    "extends": [
        "eslint:recommended",
        "plugin:import/recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
      },
    "plugins":["import"],
    "rules": {
        "indent": [
            "error"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}